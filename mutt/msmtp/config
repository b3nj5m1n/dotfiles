defaults
auth	on
tls	on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile	~/.config/msmtp/msmtp.log

account username1@email.com
host mail.email.net
port 587
from username1@email.com
user username1@email.com
passwordeval "pass username1@email.com"

account username2@email.com
host mail.email.net
port 587
from username2@email.com
user username2@email.com
passwordeval "pass username2@email.com"
